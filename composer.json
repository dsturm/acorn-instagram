{
  "name": "tiny-pixel/acorn-instagram",
  "type": "package",
  "license": "MIT",
  "description": "Social networking support for Acorn projects",
  "authors": [
    {
    "name": "Kelly Mears",
    "email": "kelly@tinypixel.dev"
    }
  ],
  "keywords": [
    "wordpress",
    "acorn",
    "sage",
    "facebook",
    "instagram",
    "twitter"
  ],
  "support": {
    "issues": "https://github.com/pixelcollective/acorn-instagram/issues"
  },
  "autoload": {
    "psr-4": {
      "TinyPixel\\Acorn\\Instagram\\": "src/"
    }
  },
  "require": {
    "php": ">=7.3",
    "raiym/instagram-php-scraper": "^0.8.34",
    "guzzlehttp/guzzle": "^6.3"
  },
  "extra": {
    "acorn": {
      "providers": [
        "TinyPixel\\Acorn\\Instagram\\Providers\\InstagramServiceProvider"
      ]
    }
  }
}
